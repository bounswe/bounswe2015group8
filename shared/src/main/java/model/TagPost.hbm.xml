<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TagPost" table="TAG_POST" schema="" catalog="test">
        <composite-id mapped="true" class="model.TagPostPK">
            <key-property name="tagId">
                <column name="TAG_ID" sql-type="bigint" not-null="true"/>
            </key-property>
            <key-property name="postId">
                <column name="POST_ID" sql-type="bigint" not-null="true"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>